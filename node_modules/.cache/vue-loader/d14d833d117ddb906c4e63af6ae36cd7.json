Card\n  },\n  data() {\n    return {\n      loadingInterval: null,\n\n      isLoading1: true\n    }\n  },\n  mounted() {\n    let count = 0\n\n    // DEMO delay for loading graphics\n    this.loadingInterval = setInterval(() => {\n      this[`isLoading${count++}`] = false\n      if (count === 4) this.clear()\n    }, 400)\n  },\n  beforeDestroy() {\n    this.clear()\n  },\n  methods: {\n    clear() {\n      clearInterval(this.loadingInterval)\n    }\n  }\n}\n</script>\n"]}]}
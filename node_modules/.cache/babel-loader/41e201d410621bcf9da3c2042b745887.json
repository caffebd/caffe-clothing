A;AACA;AANA,GAhBA;AAwBA,EAAA,OAAA,EAAA;AACA,IAAA,SADA,qBACA,MADA,EACA;AACA,WAAA,KAAA,CAAA,MAAA,GAAA,MAAA;AACA,WAAA,QAAA,CAAA,IAAA,CAAA,OAAA,GAAA,MAAA,CAFA,CAIA;;AACA,UAAA,MAAA,KAAA,IAAA,EAAA;AACA,aAAA,QAAA,CAAA,GAAA,GAAA,IAAA;AACA,OAFA,MAEA;AACA,aAAA,QAAA,CAAA,GAAA,GAAA,KAAA;AACA;AACA;AAXA;AAxBA,CAAA","sourcesContent":["<template>\n  <v-menu\n    offset-y\n    left\n    transition=\"slide-y-transition\"\n  >\n    <template v-slot:activator=\"{ on }\">\n      <v-btn text :icon=\"$vuetify.breakpoint.smAndDown\" v-on=\"on\">\n        <flag-icon :round=\"$vuetify.breakpoint.smAndDown\" :flag=\"currentLocale.flag\"></flag-icon>\n        <span v-show=\"$vuetify.breakpoint.mdAndUp && showLabel\" :class=\"[$vuetify.rtl ? 'mr-1' : 'ml-1']\">{{ currentLocale.label }}</span>\n        <v-icon v-if=\"showArrow\" right>mdi-chevron-down</v-icon>\n      </v-btn>\n    </template>\n\n    <v-list dense nav>\n      <v-list-item v-for=\"locale in availableLocales\" :key=\"locale.code\" @click=\"setLocale(locale.code)\">\n        <flag-icon :class=\"[$vuetify.rtl ? 'ml-1' : 'mr-1']\" :flag=\"locale.flag\"></flag-icon>\n        <v-list-item-title>{{ locale.label }}</v-list-item-title>\n      </v-list-item>\n    </v-list>\n  </v-menu>\n</template>\n\n<script>\nimport FlagIcon from '../common/FlagIcon'\n\n/*\n|---------------------------------------------------------------------\n| Language Switcher Component\n|---------------------------------------------------------------------\n|\n| Locale menu to choose the language based on the locales present in\n| vue-i18n locales available array\n|\n*/\nexport default {\n  components: {\n    FlagIcon\n  },\n  props: {\n    // Show dropdown arrow\n    showArrow: {\n      type: Boolean,\n      default: false\n    },\n    // Show the country label\n    showLabel: {\n      type: Boolean,\n      default: true\n    }\n  },\n  computed: {\n    currentLocale() {\n      return this.$i18n.locales.find((i) => i.code === this.$i18n.locale)\n    },\n    availableLocales () {\n      return this.$i18n.locales.filter((i) => i.code !== this.$i18n.locale)\n    }\n  },\n  methods: {\n    setLocale(locale) {\n      this.$i18n.locale = locale\n      this.$vuetify.lang.current = locale\n\n      // example on how certain languages can be RTL\n      if (locale === 'ar') {\n        this.$vuetify.rtl = true\n      } else {\n        this.$vuetify.rtl = false\n      }\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/components/toolbar"}]}
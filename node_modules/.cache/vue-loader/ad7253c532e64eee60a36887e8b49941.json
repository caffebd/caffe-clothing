  <v-btn color=\"primary\">\n        Create User\n      </v-btn>\n    </div>\n\n    <v-card>\n      <!-- users list -->\n      <v-row dense class=\"pa-2 align-center\">\n        <v-col cols=\"6\">\n          <v-menu offset-y left>\n            <template v-slot:activator=\"{ on }\">\n              <transition name=\"slide-fade\" mode=\"out-in\">\n                <v-btn v-show=\"selectedUsers.length > 0\" v-on=\"on\">\n                  Actions\n                  <v-icon right>mdi-menu-down</v-icon>\n                </v-btn>\n              </transition>\n            </template>\n            <v-list dense>\n              <v-list-item @click>\n                <v-list-item-title>Verify</v-list-item-title>\n              </v-list-item>\n              <v-list-item @click>\n                <v-list-item-title>Disable</v-list-item-title>\n              </v-list-item>\n              <v-divider></v-divider>\n              <v-list-item @click>\n                <v-list-item-title>Delete</v-list-item-title>\n              </v-list-item>\n            </v-list>\n          </v-menu>\n\n        </v-col>\n        <v-col cols=\"6\" class=\"d-flex text-right align-center\">\n          <v-text-field\n            v-model=\"searchQuery\"\n            append-icon=\"mdi-magnify\"\n            class=\"flex-grow-1 mr-md-2\"\n            solo\n            hide-details\n            dense\n            clearable\n            placeholder=\"e.g. filter for id, email, name, etc\"\n            @keyup.enter=\"searchUser(searchQuery)\"\n          ></v-text-field>\n          <v-btn\n            :loading=\"isLoading\"\n            icon\n            small\n            class=\"ml-2\"\n            @click\n          >\n            <v-icon>mdi-refresh</v-icon>\n          </v-btn>\n        </v-col>\n      </v-row>\n\n      <v-data-table\n        v-model=\"selectedUsers\"\n        show-select\n        :headers=\"headers\"\n        :items=\"users\"\n        :search=\"searchQuery\"\n        class=\"flex-grow-1\"\n      >\n        <template v-slot:item.id=\"{ item }\">\n          <div class=\"font-weight-bold\"># <copy-label :text=\"item.id + ''\" /></div>\n        </template>\n\n        <template v-slot:item.email=\"{ item }\">\n          <div class=\"d-flex align-center py-1\">\n            <v-avatar size=\"32\" class=\"elevation-1 grey lighten-3\">\n              <v-img :src=\"item.avatar\" />\n            </v-avatar>\n            <div class=\"ml-1 caption font-weight-bold\">\n              <copy-label :text=\"item.email\" />\n            </div>\n          </div>\n        </template>\n\n        <template v-slot:item.verified=\"{ item }\">\n          <v-icon v-if=\"item.verified\" small color=\"success\">\n            mdi-check-circle\n          </v-icon>\n          <v-icon v-else small>\n            mdi-circle-outline\n          </v-icon>\n        </template>\n\n        <template v-slot:item.disabled=\"{ item }\">\n          <div>{{ item.disabled.toString() | capitalize }}</div>\n        </template>\n\n        <template v-slot:item.role=\"{ item }\">\n          <v-chip\n            label\n            small\n            class=\"font-weight-bold\"\n            :color=\"item.role === 'ADMIN' ? 'primary' : undefined\"\n          >{{ item.role | capitalize }}</v-chip>\n        </template>\n\n        <template v-slot:item.created=\"{ item }\">\n          <div>{{ item.created | formatDate('ll') }}</div>\n        </template>\n\n        <template v-slot:item.lastSignIn=\"{ item }\">\n          <div>{{ item.lastSignIn | formatDate('lll') }}</div>\n        </template>\n\n        <template v-slot:item.action=\"{ }\">\n          <div class=\"actions\">\n            <v-btn icon to=\"/users/edit\">\n              <v-icon>mdi-open-in-new</v-icon>\n            </v-btn>\n          </div>\n        </template>\n      </v-data-table>\n    </v-card>\n  </div>\n</template>\n\n<script>\nimport users from './content/users'\nimport CopyLabel from '../../components/common/CopyLabel'\n\nexport default {\n  components: {\n    CopyLabel\n  },\n  data() {\n    return {\n      isLoading: false,\n      breadcrumbs: [{\n        text: 'Users',\n        disabled: false,\n        href: '#'\n      }, {\n        text: 'List'\n      }],\n\n      searchQuery: '',\n      selectedUsers: [],\n      headers: [\n        { text: 'Id', align: 'left', value: 'id' },\n        { text: 'Email', value: 'email' },\n        { text: 'Verified', value: 'verified' },\n        { text: 'Name', align: 'left', value: 'name' },\n        { text: 'Role', value: 'role' },\n        { text: 'Created', value: 'created' },\n        { text: 'Last SignIn', value: 'lastSignIn' },\n        { text: 'Disabled', value: 'disabled' },\n        { text: '', sortable: false, align: 'right', value: 'action' }\n      ],\n\n      users\n    }\n  },\n  watch: {\n    selectedUsers(val) {\n\n    }\n  },\n  methods: {\n    searchUser() {},\n    open() {}\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.slide-fade-enter-active {\n  transition: all 0.3s ease;\n}\n.slide-fade-leave-active {\n  transition: all 0.3s cubic-bezier(1, 0.5, 0.8, 1);\n}\n.slide-fade-enter,\n.slide-fade-leave-to {\n  transform: translateX(10px);\n  opacity: 0;\n}\n</style>\n"]}]}
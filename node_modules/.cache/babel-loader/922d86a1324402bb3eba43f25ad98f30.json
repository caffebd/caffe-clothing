;AAGA,QAAA,SAAA,EAAA;AAHA,OAJA,CAvBA;AAiCA;AACA,MAAA,KAAA,EAAA;AACA,QAAA,QAAA,EAAA,kBAAA,KAAA;AAAA,iBAAA,KAAA,IAAA,OAAA,CAAA,KAAA,CAAA,IAAA,UAAA;AAAA;AADA;AAlCA,KAAA;AAsCA,GAxCA;AA0CA,EAAA,OAAA,EAAA;AACA,IAAA,0BADA,wCACA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,KAAA,CAAA,SAAA,GAAA,IAAA;AADA;AAAA;AAAA,uBAGA,QAAA,CACA,IADA,GAEA,0BAFA,CAEA,KAAA,CAAA,KAAA,CAAA,WAAA,EAFA,EAEA,KAAA,CAAA,QAFA,CAHA;;AAAA;AAGA,gBAAA,OAHA;AAAA;AAAA,uBAOA,EAAA,CAAA,UAAA,CAAA,WAAA,EAAA,GAAA,CAAA,OAAA,CAAA,IAAA,CAAA,GAAA,EAAA,GAAA,EAPA;;AAAA;AAOA,gBAAA,MAPA;AASA,gBAAA,QATA,GASA,MAAA,CAAA,IAAA,EATA;;AAWA,gBAAA,KAAA,CAAA,MAAA,CAAA,QAAA,CAAA,kBAAA,EAAA;AACA,kBAAA,EAAA,EAAA,MAAA,CAAA,EADA;AAEA,kBAAA,IAAA,EAAA,QAAA,CAAA,IAFA;AAGA,kBAAA,KAAA,EAAA,QAAA,CAAA,KAHA;AAIA,kBAAA,SAAA,EAAA,QAAA,CAAA,SAAA,IAAA,EAJA;AAKA,kBAAA,YAAA,EAAA,QAAA,CAAA,YAAA,IAAA;AALA,iBAAA;;AAQA,gBAAA,KAAA,CAAA,OAAA,CAAA,OAAA,CAAA;AAAA,kBAAA,IAAA,EAAA;AAAA,iBAAA;;AAnBA;AAAA;;AAAA;AAAA;AAAA;AAqBA,gBAAA,KAAA,CAAA,QAAA,GAAA,mCAAA;;AACA,oBAAA,YAAA,OAAA,EAAA;AACA,kBAAA,KAAA,CAAA,QAAA,GAAA,YAAA,OAAA;AACA;;AAxBA;AA0BA,gBAAA,KAAA,CAAA,SAAA,GAAA,KAAA;;AA1BA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA2BA,KA5BA;AA8BA,IAAA,MA9BA,oBA8BA;AACA,UAAA,KAAA,KAAA,CAAA,IAAA,CAAA,QAAA,EAAA,EAAA;AACA,aAAA,SAAA,GAAA,IAAA;AACA,aAAA,gBAAA,GAAA,IAAA;AACA,aAAA,MAAA,CAAA,KAAA,KAAA,EAAA,KAAA,QAAA;AACA;AACA,KApCA;AAqCA,IAAA,MArCA,kBAqCA,KArCA,EAqCA,QArCA,EAqCA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,GAAA;AACA,KAvCA;AAwCA,IAAA,cAxCA,0BAwCA,QAxCA,EAwCA,CAAA,CAxCA;AAyCA,IAAA,WAzCA,yBAyCA;AACA,WAAA,KAAA,GAAA,KAAA;AACA,WAAA,aAAA,GAAA,EAAA;AAEA,WAAA,aAAA,GAAA,KAAA;AACA,WAAA,qBAAA,GAAA,EAAA;AACA;AA/CA;AA1CA,CAAA","sourcesContent":["<template>\n  <div>\n    <v-card class=\"text-center pa-1\">\n      <v-card-title class=\"justify-center display-1 mb-2\">Welcome</v-card-title>\n      <v-card-subtitle>Sign in to your account</v-card-subtitle>\n\n      <!-- sign in form -->\n      <v-card-text>\n        <v-form ref=\"form\" v-model=\"isFormValid\" lazy-validation>\n          <v-text-field\n            v-model=\"email\"\n            :rules=\"[rules.required]\"\n            :validate-on-blur=\"false\"\n            :error=\"error\"\n            :label=\"$t('login.email')\"\n            name=\"email\"\n            outlined\n            @keyup.enter=\"signInWithEmailAndPassword\"\n            @change=\"resetErrors\"\n          ></v-text-field>\n\n          <v-text-field\n            v-model=\"password\"\n            :append-icon=\"showPassword ? 'mdi-eye' : 'mdi-eye-off'\"\n            :rules=\"[rules.required]\"\n            :type=\"showPassword ? 'text' : 'password'\"\n            :error=\"error\"\n            :error-messages=\"errorMessages\"\n            :label=\"$t('login.password')\"\n            name=\"password\"\n            outlined\n            @change=\"resetErrors\"\n            @keyup.enter=\"signInWithEmailAndPassword\"\n            @click:append=\"showPassword = !showPassword\"\n          ></v-text-field>\n\n          <v-btn\n            :loading=\"isLoading\"\n            :disabled=\"isSignInDisabled\"\n            block\n            x-large\n            color=\"primary\"\n            @click=\"signInWithEmailAndPassword\"\n          >{{ $t('login.button') }}</v-btn>\n\n          <div class=\"caption font-weight-bold text-uppercase my-3\">{{ $t('login.orsign') }}</div>\n\n          <!-- external providers list -->\n          <v-btn\n            v-for=\"provider in providers\"\n            :key=\"provider.id\"\n            :loading=\"provider.isLoading\"\n            :disabled=\"isSignInDisabled\"\n            class=\"mb-2 primary lighten-2 primary--text text--darken-3\"\n            block\n            x-large\n            to=\"/\"\n          >\n            <v-icon small left>mdi-{{ provider.id }}</v-icon>\n            {{ provider.label }}\n          </v-btn>\n\n          <div v-if=\"errorProvider\" class=\"error--text\">{{ errorProviderMessages }}</div>\n\n          <div class=\"mt-5\">\n            <router-link to=\"/auth/forgot-password\">\n              {{ $t('login.forgot') }}\n            </router-link>\n          </div>\n        </v-form>\n      </v-card-text>\n    </v-card>\n\n    <div class=\"text-center mt-6\">\n      {{ $t('login.noaccount') }}\n      <router-link to=\"/auth/signup\" class=\"font-weight-bold\">\n        {{ $t('login.create') }}\n      </router-link>\n    </div>\n  </div>\n</template>\n\n<script>\n/*\n|---------------------------------------------------------------------\n| Sign In Page Component\n|---------------------------------------------------------------------\n|\n| Sign in template for user authentication into the application\n|\n*/\nimport firebase from 'firebase'\nimport { db } from '../../main'\nexport default {\n  data() {\n    return {\n      // sign in buttons\n      isLoading: false,\n      isSignInDisabled: false,\n\n      // form\n      isFormValid: true,\n      email: '',\n      password: '',\n\n      errorMsg: '',\n      form: false,\n\n      // form error\n      error: false,\n      errorMessages: '',\n\n      errorProvider: false,\n      errorProviderMessages: '',\n\n      // show password field\n      showPassword: false,\n\n      providers: [{\n        id: 'google',\n        label: 'Google',\n        isLoading: false\n      }, {\n        id: 'facebook',\n        label: 'Facebook',\n        isLoading: false\n      }],\n\n      // input rules\n      rules: {\n        required: (value) => (value && Boolean(value)) || 'Required'\n      }\n    }\n  },\n\n  methods: {\n    async signInWithEmailAndPassword() {\n      this.isLoading = true\n      try {\n        const authRes = await firebase\n          .auth()\n          .signInWithEmailAndPassword(this.email.toLowerCase(), this.password)\n\n        const dbUser = await db.collection('testusers').doc(authRes.user.uid).get()\n\n        const userData = dbUser.data()\n\n        this.$store.dispatch('user/setUserData', {\n          id: dbUser.id,\n          name: userData.name,\n          email: userData.email,\n          partnerId: userData.partnerId || '',\n          movieApiPage: userData.movueApiPage || 1\n        })\n\n        this.$router.replace({ name: 'dashboard-analytics' })\n      } catch (error) {\n        this.errorMsg = 'An unexpected error has occurred!'\n        if (error.message) {\n          this.errorMsg = error.message\n        }\n      }\n      this.isLoading = false\n    },\n\n    submit() {\n      if (this.$refs.form.validate()) {\n        this.isLoading = true\n        this.isSignInDisabled = true\n        this.signIn(this.email, this.password)\n      }\n    },\n    signIn(email, password) {\n      this.$router.push('/')\n    },\n    signInProvider(provider) {},\n    resetErrors() {\n      this.error = false\n      this.errorMessages = ''\n\n      this.errorProvider = false\n      this.errorProviderMessages = ''\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/pages/auth"}]}